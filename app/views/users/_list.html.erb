<h1>NEW Book</h1>


    <ul>
    <% book.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>

<!-- form_for部分 -->
   <%= form_for book do |f| %>

   <h4>Title</h4>

   <%= f.text_field :title %>

   <h4>Body</h4>

   <%= f.text_area :body %>
   <%= f.submit 'Create Book' %>

<% end %>